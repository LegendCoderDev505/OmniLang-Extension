{
  "name": "omnilang-framework",
  "displayName": "OmniLang Framework",
  "description": "OmniLang Framework Support for OmniLang",
  "version": "0.0.5",
  "publisher": "LegendCoderDev505",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "omnilang",
      "aliases": ["OmniLang", "omnilang"],
      "extensions": [".omni"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "omnilang",
      "scopeName": "source.omnilang",
      "path": "./syntaxes/omnilang.tmLanguage.json"
    }]
  },
  "scripts": {
    "package": "vsce package --no-dependencies --baseContentUrl https://open-vsx.org --baseImagesUrl https://open-vsx.org",
    "vscode:prepublish": "echo \"Skipping prepublish\""
  },
  "devDependencies": {
    "@vscode/vsce": "^2.21.0"
  }
}